{
    "name": "yii2lib/example",
    "description": "Yii2 framework library example",
    "keywords": [
        "yii2",
        "library",
        "example"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/yii2lib/example",
    "support": {
        "issues": "https://github.com/yii2lib/example/issues",
        "forum": "https://github.com/yii2lib/example/issues",
        "wiki": "https://github.com/yii2lib/example/wiki",
        "source": "https://github.com/yii2lib/example"
    },
    "authors": [
        {
            "name": "Ruitang Du",
            "email": "frontbear@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "~2.0.14"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0.0 || ~6.5.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "yii2lib\\example\\": "src"
        }
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
